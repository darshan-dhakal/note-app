generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
    // url      = env("DATABASE_URL")  // Make sure DATABASE_URL is in .env


  
}

model Note {
  id        Int      @id @default(autoincrement())
  title     String
  content   String
  createdAt DateTime @default(now())
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
  name     String
}
